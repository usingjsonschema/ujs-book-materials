{
  "$schema":"http://json-schema.org/draft-04/schema#",
  "title":"Inventory",
  "description":"Inventory with current stock count",

  "type":"array",
  "items":
  {
    "properties":
    {
      "id":{ "type":"string" },
      "count":{ "type":"integer", "minimum":0 }
    },
    "required":["id", "count"],
    "additionalProperties":false
  }
}
