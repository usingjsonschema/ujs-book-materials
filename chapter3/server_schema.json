{
  "$schema":"http://json-schema.org/draft-04/schema#",
  "title":"Server",
  "description":"Server name and IP address, port",

  "type":"object",
  "properties":
  {
    "name":{"type":"string"},
    "address":{"type":"string"},
    "port":{"type":"integer", "minimum":0, "maximum":65535}
  },
  "additionalProperties":false,
  "required":["name", "address", "port"]
}
