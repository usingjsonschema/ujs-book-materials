{
  "$schema":"http://json-schema.org/draft-04/schema#",
  "title":"Postal address for Canada",
  "description":"Postal address for Canada",

  "type":"object",
  "properties":
  {
    "address":
    {
      "type":"object",
      "properties":
      {
        "name":{"type":"string"},
        "number":{"type":"string"},
        "street":{"type":"string"},
        "street2":{"type":"string"},
        "city":{"type":"string"},
        "province":
        {
            "$ref":"http://localhost:8081/schema/postCanadaCommon_schema.json#/definitions/CAN_province"
        },
        "postalCode":
        {
            "$ref":"http://localhost:8081/schema/postCanadaCommon_schema.json#/definitions/CAN_postalCode"
        }
      },
      "additionalProperties":false,
      "required":["name", "number", "street", "city", "province", "postalCode"]
    }
  }
}
